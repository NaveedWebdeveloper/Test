<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<description>TemplaVoila Notes locallang</description>
		<type>module</type>
		<fileId>EXT:rlmp_tvnotes/locallang.xml</fileId>
		<csh_table>_nothing_</csh_table>
		<keep_original_text>1</keep_original_text>
		<ext_filename_template>EXT:rlmp_tvnotes/###LANGKEY###.locallang.xml</ext_filename_template>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="note_status_read">read note</label>
			<label index="note_status_unread">unread note</label>
			<label index="note_status_empty">empty note</label>
			<label index="action_close">close note</label>
			<label index="action_edit">edit note</label>
			<label index="action_save">save note</label>
			<label index="action_delete">delete note</label>
			<label index="action_confirm_delete">Are you sure you want to delete this note?</label>
			<label index="untitled">Untitled</label>
		</languageKey>
		<languageKey index="de">EXT:rlmp_tvnotes/de.locallang.xml</languageKey>
	</data>
</T3locallang>